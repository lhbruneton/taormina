    "husky": "^7.0.4",
    "jasmine-marbles": "0.9.2",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-jasmine2": "29.7.0",
    "jest-preset-angular": "14.1.1",
    "leasot": "^12.0.0",
    "lint-staged": "^12.1.2",
    "ng-mocks": "^14.13.0",
    "nx": "19.6.6",
    "prettier": "2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs-marbles": "^7.0.1",
    "stylelint": "^13.13.0",
    "stylelint-config-standard": "^22.0.0",
