    "@nx/jest": "19.6.6",
    "@nx/storybook": "19.6.6",
    "@nx/workspace": "19.6.6",
    "@schematics/angular": "18.2.3",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/angular": "^8.2.9",
    "@storybook/cli": "^8.2.9",
    "@types/jest": "29.5.12",
    "@types/node": "18.16.9",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "codelyzer": "^6.0.0",
