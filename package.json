{
  "name": "taormina",
  "version": "0.2.0-0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "nx run taormina-duel:serve",
    "start:storybook:ui-card": "start-storybook --config-dir libs/ui-card/.storybook",
    "build": "nx run taormina-duel:build:production --skip-nx-cache",
    "build:gh-pages": "nx run taormina-duel:build:production --output-path docs --base-href /taormina/ --skip-nx-cache",
    "test:all": "nx run-many --target=test --all --skip-nx-cache --parallel=1",
    "lint:all": "nx run-many --target=lint --all --skip-nx-cache",
    "lint:styles": "stylelint \"apps/**/*.css\"",
    "e2e": "ng e2e",
    "todo": "leasot '**/*.ts' --ignore 'node_modules,**/src/support' --tags @ts-expect-error,eslint-disable-next-line,prettier-ignore",
    "todo:gh-actions": "leasot '**/*.ts' --ignore 'node_modules,**/src/support' --tags @ts-expect-error,eslint-disable-next-line,prettier-ignore -x --reporter markdown > TODO.md",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "updtr": "updtr -t \"npm run test:all\""
  },
  "private": true,
  "sideEffects": false,
  "dependencies": {
    "@angular/animations": "14.0.4",
    "@angular/cdk": "14.0.4",
    "@angular/common": "14.0.4",
    "@angular/compiler": "14.0.4",
    "@angular/core": "14.0.4",
    "@angular/forms": "14.0.4",
    "@angular/material": "14.0.4",
    "@angular/platform-browser": "14.0.4",
    "@angular/platform-browser-dynamic": "14.0.4",
    "@angular/router": "14.0.4",
    "@ngrx/effects": "14.0.1",
    "@ngrx/entity": "14.0.1",
    "@ngrx/router-store": "14.0.1",
    "@ngrx/store": "14.0.1",
    "@nrwl/angular": "14.4.0",
    "array-shuffle": "^2.0.0",
    "eslint-plugin-html": "^6.1.1",
    "rxjs": "^6.6.6",
    "tslib": "^2.1.0",
    "uuid": "^8.3.2",
    "zone.js": "0.11.4",
    "@storybook/core-server": "6.4.22"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.0.4",
    "@angular-eslint/eslint-plugin": "14.0.0",
    "@angular-eslint/eslint-plugin-template": "14.0.0",
    "@angular-eslint/template-parser": "14.0.0",
    "@angular/cli": "~14.0.0",
    "@angular/compiler-cli": "14.0.4",
    "@angular/language-service": "14.0.4",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@ngrx/schematics": "14.0.1",
    "@ngrx/store-devtools": "14.0.1",
    "@nrwl/cli": "14.4.0",
    "@nrwl/cypress": "14.4.0",
    "@nrwl/eslint-plugin-nx": "14.4.0",
    "@nrwl/jest": "14.4.0",
    "@nrwl/nx-cloud": "14.2.0",
    "@nrwl/storybook": "14.4.0",
    "@nrwl/workspace": "14.4.0",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/angular": "6.4.22",
    "@storybook/builder-webpack5": "6.4.22",
    "@storybook/cli": "^6.4.9",
    "@storybook/manager-webpack5": "6.4.22",
    "@types/jest": "27.4.1",
    "@types/node": "^16.11.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "codelyzer": "^6.0.0",
    "commitizen": "^4.2.3",
    "cypress": "^7.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "10.0.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^35.5.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "husky": "^7.0.4",
    "jasmine-marbles": "0.8.4",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.2",
    "leasot": "^12.0.0",
    "lint-staged": "^12.1.2",
    "prettier": "2.7.1",
    "rxjs-marbles": "^6.0.1",
    "stylelint": "^13.13.0",
    "stylelint-config-standard": "^22.0.0",
    "ts-jest": "27.1.4",
    "ts-node": "10.8.2",
    "tslint": "~6.1.0",
    "typescript": "4.7.4",
    "nx": "14.4.0"
  }
}
