    "eslint-plugin-storybook": "^0.6.13",
    "husky": "^7.0.4",
    "jasmine-marbles": "0.9.2",
    "jest": "29.6.4",
    "jest-environment-jsdom": "29.6.4",
    "jest-jasmine2": "28.1.1",
    "jest-preset-angular": "13.1.1",
    "leasot": "^12.0.0",
    "lint-staged": "^12.1.2",
    "ng-mocks": "^14.11.0",
    "nx": "^16.8.0",
    "nx-cloud": "16.3.0",
    "prettier": "2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs-marbles": "^7.0.1",
    "stylelint": "^13.13.0",
    "stylelint-config-standard": "^22.0.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tslint": "~6.1.0",
    "typescript": "5.1.6"
  }
}
