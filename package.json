{
  "name": "taormina",
  "version": "0.2.0-0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js",
    "nx": "nx",
    "start": "nx run taormina-duel:serve",
    "start:storybook:ui-card": "storybook dev --config-dir libs/ui-card/.storybook",
    "build": "nx run taormina-duel:build:production --skip-nx-cache",
    "build:gh-pages": "nx run taormina-duel:build:production --output-path docs --base-href /taormina/ --skip-nx-cache",
    "test:all": "nx run-many --target=test --all --skip-nx-cache --parallel=1",
    "lint:all": "nx run-many --target=lint --all --skip-nx-cache",
    "lint:styles": "stylelint \"apps/**/*.css\"",
    "e2e": "ng e2e",
    "todo": "leasot '**/*.ts' --ignore 'node_modules,**/src/support' --tags @ts-expect-error,eslint-disable-next-line,prettier-ignore",
    "todo:gh-actions": "leasot '**/*.ts' --ignore 'node_modules,**/src/support' --tags @ts-expect-error,eslint-disable-next-line,prettier-ignore -x --reporter markdown > TODO.md",
    "compodoc": "compodoc -p tsconfig.doc.json",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "updtr": "updtr -t \"npm run test:all\""
  },
  "private": true,
  "sideEffects": false,
  "dependencies": {
    "@angular/animations": "16.2.4",
    "@angular/cdk": "16.2.2",
    "@angular/common": "16.2.4",
    "@angular/compiler": "16.2.4",
    "@angular/core": "16.2.4",
    "@angular/forms": "16.2.4",
    "@angular/material": "16.2.2",
    "@angular/platform-browser": "16.2.4",
    "@angular/platform-browser-dynamic": "16.2.4",
    "@angular/router": "16.2.4",
    "@ngrx/effects": "^16.2.0",
    "@ngrx/entity": "^16.2.0",
    "@ngrx/router-store": "^16.2.0",
    "@ngrx/store": "^16.2.0",
    "@nx/angular": "^16.8.0",
    "@storybook/core-server": "7.4.0",
    "array-shuffle": "^2.0.0",
    "eslint-plugin-html": "^6.1.1",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "uuid": "^8.3.2",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.1",
    "@angular-devkit/core": "16.2.1",
    "@angular-devkit/schematics": "16.2.1",
    "@angular-eslint/eslint-plugin": "16.1.2",
    "@angular-eslint/eslint-plugin-template": "16.1.2",
    "@angular-eslint/template-parser": "16.1.2",
    "@angular/cli": "^16.2.1",
    "@angular/compiler-cli": "16.2.4",
    "@angular/language-service": "16.2.4",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@compodoc/compodoc": "^1.1.21",
    "@ngrx/schematics": "^16.2.0",
    "@ngrx/store-devtools": "^16.2.0",
    "@nx/cypress": "^16.8.0",
    "@nx/eslint-plugin": "^16.8.0",
    "@nx/jest": "^16.8.0",
    "@nx/storybook": "^16.8.0",
    "@nx/workspace": "^16.8.0",
    "@schematics/angular": "^16.2.1",
    "@storybook/addon-essentials": "7.4.0",
    "@storybook/angular": "7.4.0",
    "@storybook/cli": "~7.4.0",
    "@types/jest": "29.5.4",
    "@types/node": "^16.11.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "codelyzer": "^6.0.0",
    "commitizen": "^4.3.0",
    "cypress": "^7.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "10.0.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "2.14.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^35.5.1",
